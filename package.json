    "artificial-intelligence",
    "cultural-intelligence",
    "metaverse",
    "research-ai",
    "content-generation",
    "tourism-intelligence",
    "job-market-intelligence",
    "security-mapping",
    "business-intelligence",
    "multi-country-ai",
    "cultural-adaptation",
    "nextjs",
    "react",
    "typescript",
    "supabase",
    "firebase"
  ],
  "author": {
    "name": "Cerebrum IP Solutions Ltd",
